                    <Font size="16.0" />
                </font></TextArea>
            <HBox prefHeight="30.0" prefWidth="200.0">
                <children>
                    <TextField fx:id="textField" onAction="#onSubmit" prefHeight="30.0" prefWidth="200.0">
